<?xml version="1.0" encoding="UTF-8"?>
<proxy name="buffer-loan-proposals" startOnLoad="true" transports="https http" xmlns="http://ws.apache.org/ns/synapse">
    <target>
        <endpoint>
            <address uri="jms:/loan-proposal-queue1?transport.jms.ConnectionFactoryJNDIName=QueueConnectionFactory&amp;java.naming.factory.initial=org.apache.activemq.jndi.ActiveMQInitialContextFactory&amp;java.naming.provider.url=tcp://localhost:61616&amp;transport.jms.DestinationType=queue">
                <suspendOnFailure>
                    <initialDuration>0</initialDuration>
                    <progressionFactor>1</progressionFactor>
                    <maximumDuration>0</maximumDuration>
                </suspendOnFailure>
                <markForSuspension>
                    <retriesBeforeSuspension>0</retriesBeforeSuspension>
                </markForSuspension>
            </address>
        </endpoint>
        <inSequence>
            <property name="transport.jms.ContentTypeProperty" scope="axis2" type="STRING" value="Content-Type"/>
            <property name="JMS_WAIT_REPLY" scope="axis2" type="STRING" value="600000"/>
            <property name="JMS_TIME_TO_LIVE" scope="axis2" type="STRING" value="600000"/>
            <property action="remove" name="transportNonBlocking" scope="axis2"/>
        </inSequence>
        <outSequence>
            <property action="remove" name="transportNonBlocking" scope="axis2"/>
            <property action="remove" name="TRANSPORT_HEADERS" scope="axis2"/>
            <property name="JMS_WAIT_REPLY" scope="axis2" type="STRING" value="60000"/>
            <property name="JMS_TIME_TO_LIVE" scope="axis2" type="STRING" value="20000"/>
            <property name="messageType" scope="axis2" type="STRING" value="application/json"/>
            <send/>
        </outSequence>
        <faultSequence/>
    </target>
</proxy>
